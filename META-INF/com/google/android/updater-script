set_perm(0, 0, 0644, "/system/priv-app/GoogleDialer/lib/arm64/libsrtp.so");
set_perm(0, 0, 0644, "/system/priv-app/GoogleMessages/GoogleMessages.apk");
endif;
#################################################################
# SuperSU

if
file_getprop("/tmp/aroma-data/mods.prop","selected.1") == "2"
then
unmount("/data");
unmount("/cache");
unmount("/preload");
unmount("/system");
ui_print("Installing SuperSU...");
package_extract_dir("system/supersu", "/tmp/supersu");
run_program("/sbin/busybox", "unzip", "/tmp/supersu/supersu.zip", "META-INF/com/google/android/update-binary", "-d", "/tmp/supersu");
run_program("/sbin/busybox", "sh", "/tmp/supersu/META-INF/com/google/android/update-binary", "null", "1", "/tmp/supersu/supersu.zip");
delete_recursive("/tmp/supersu");
delete_recursive("/system/supersu");
ui_print("SuperSU installed successfully");
endif;

#################################################################
# Magisk

if
file_getprop("/tmp/aroma-data/mods.prop","selected.1") == "3"
then

ui_print("Installing proper Toolbox...");
delete_recursive("/system/priv-app/Toolbox.apk");
package_extract_dir("jdc/magisk/packages/jdctoolbox", "/system/priv-app/Toolbox");
set_perm(0, 0, 0644, "/system/priv-app/Toolbox/Toolbox.apk");

ui_print("Installing Magisk binaries...");
package_extract_dir("jdc/magisk/flashable", "/tmp/magisk/flashable");
run_program("/sbin/busybox", "unzip", "/tmp/magisk/flashable/Magisk.zip", "META-INF/com/google/android/update-binary", "-d", "/tmp/magisk/flashable");
run_program("/sbin/busybox", "sh", "/tmp/magisk/flashable/META-INF/com/google/android/update-binary", "null", "1", "/tmp/magisk/flashable/Magisk.zip");

delete_recursive("/tmp/magisk");
ui_print("Magisk installed successfully");

endif;

#################################################################
# Privacy Guard

if
file_getprop("/tmp/aroma-data/mods.prop","selected.1") == "1"
then
ui_print("Privacy guard will be installed");
endif;

#################################################################
# Finish Script

ui_print("Cleaning temporary files");
delete_recursive("/tmpJDC");
delete_recursive("/tmp/boot.img");
unmount("/system");
ui_print("__________________________________________________");
ui_print("INSTALLATION COMPLETE!");
ui_print(" ");
ui_print(" ");
ui_print(" ");
ui_print(" ");
ui_print(" ");
ui_print("              Press Finish to exit and continue to TWRP");
ui_print(" ");
set_progress(1.0);
endif;

