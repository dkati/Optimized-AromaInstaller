file_getprop("/tmp/aroma-data/menu.prop","selected") == "2"
then

ui_print("Mounting /system...");
run_program("/sbin/mount", "-t", "auto", "/system");

if
file_getprop("/tmp/aroma-data/remove.prop","selected.1") == "2"
then
	ui_print("Removing STweaks...");
	delete_recursive("/system/priv-app/STweaks");
	delete_recursive("/data/.b--b");
endif;

if
file_getprop("/tmp/aroma-data/remove.prop","selected.2") == "2"
then
	ui_print("Removing Viper4Arise...");
	delete_recursive("/data/app/ViPER4ARISE");
	delete_recursive("/data/app/Viper4ARISE");
	delete_recursive("/data/app/ViPER4Arise");
	
	delete_recursive("/data/data/ViPER4ARISE");
	delete_recursive("/data/data/Viper4ARISE");
	delete_recursive("/data/data/ViPER4Arise");
	
endif;

if
file_getprop("/tmp/aroma-data/remove.prop","selected.3") == "2"
then
	ui_print("Removing MiXplorer...");
	delete_recursive("/system/priv-app/MiXplorer");
endif;

if
file_getprop("/tmp/aroma-data/remove.prop","selected.4") == "2"
then
	ui_print("Removing AOSP Camera...");
	delete_recursive("/system/app/Camera2");
	delete_recursive("/system/lib/libjni_tinyplanet.so");
	delete_recursive("/system/lib/libjni_jpegutil.so");
endif;

